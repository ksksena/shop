<Window x:Name="mainWindow" x:Class="shop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:shop"
        mc:Ignorable="d"
        Title="Канцелярский Магазин" Height="550" Width="800" Icon="/pencil.jpg"
        MinWidth="400"
        MinHeight="400"
        MaxWidth="800"
        MaxHeight="550">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="149*"/>
            <ColumnDefinition Width="58*"/>
            <ColumnDefinition Width="593*"/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="frameMain" Content="Frame" Foreground="White" NavigationUIVisibility="Hidden" Margin="0,-3,-1,2" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="3">
            <Frame.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="0.343"/>
                    <RotateTransform/>
                    <TranslateTransform Y="2.606"/>
                </TransformGroup>
            </Frame.RenderTransform>
        </Frame>

    </Grid>
</Window>
